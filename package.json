{
  "name": "ai-discovery",
  "version": "1.0.0",
  "description": "AI Discovery - Intelligent AI Tools Directory and Review Platform",
  "main": "index.js",
  "scripts": {
    "dev": "hugo server -D --port 3000 --bind 0.0.0.0",
    "build": "hugo --minify --environment production",
    "preview": "hugo server --minify --environment production --port 3001",
    "deploy": "vercel --prod",
    "lint": "echo 'No JS linting configured'",
    "test": "python test_keyword_analyzer.py && python test_content_generator.py"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fzero1925/ai-discovery.git"
  },
  "keywords": [
    "AI tools",
    "artificial intelligence",
    "tool directory",
    "reviews",
    "hugo",
    "static site",
    "automation"
  ],
  "author": "AI Discovery Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fzero1925/ai-discovery/issues"
  },
  "homepage": "https://github.com/Fzero1925/ai-discovery#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {},
  "dependencies": {},
  "vercel": {
    "version": 2,
    "build": {
      "env": {
        "HUGO_VERSION": "0.149.0"
      }
    }
  }
}